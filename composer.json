{
    "name": "xenokore/web-skeleton",
    "description": "A skeleton project for a Xeno web app",
    "type": "project",
    "version": "0.3.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Controller\\": "controllers/"
        },
        "classmap": [
            "src/",
            "controllers/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "xenokore/utility": "^1.0",
        "psr/http-message": "^1.0",
        "psr/container": "^1.0",
        "php-di/php-di": "^6.3",
        "twig/twig": "^3.3",
        "xenokore/app": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "escapestudios/symfony2-coding-standard": "^3.12",
        "psy/psysh": "^0.10.8"
    }
}
